{
  "name": "@mhiseg/esm-styleguide-app",
  "version": "0.0.1",
  "description": "reference application based style guide for using with OpenMRS micro frontend",
  "repo": "/openmrs/frontend/mhiseg-esm-styleguide-app",
  "main": "dist/mhiseg-esm-styleguide.js",
  "scripts": {
    "serve": "webpack-dev-server",
    "start": "webpack-dev-server",
    "build": "webpack",
    "copy": "copyfiles -e dist/*.txt -f dist/*.* $npm_package_repo-$npm_package_version/",
    "deploy": "yarn build && yarn copy",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openmrs/style-referenceapplication": "^0.1.0"
  },
  "directories": {
    "lib": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-refapp-styleguide"
  },
  "keywords": [
    "openmrs",
    "refapp",
    "styleguide"
  ],
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.2.0",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.0.0",
    "systemjs-webpack-interop": "^1.1.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.11.0"
  }
}
